cd $work_path

confdir=@config_dir@
del_nat=`echo $nat_name | sed s/:/\ /`

${cat_cmd} ${confdir}/netustad.natd | sed /"$del_nat"/d > /tmp/netustad.natd
mv /tmp/netustad.natd ${confdir}

#==================
#== Call shownat ==
#==================
./shownat
